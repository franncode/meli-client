{
	"version": 2,
	"builds": [
		{
			"src": "next.config.js",
			"use": "@now/next"
		}
	],
	"routes": [
		{
			"src": "^/service-worker.js$",
			"dest": "/_next/public/service-worker.js",
			"headers": {
				"cache-control": "public, max-age=43200, immutable",
				"Service-Worker-Allowed": "/"
			}
		}
	]
}
